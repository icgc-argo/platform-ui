[ignore]
.*/node_modules/*

[include]

[libs]

[options]

module.name_mapper='^global\(.*\)$' -> '<PROJECT_ROOT>/global/\1'
module.name_mapper='^components\(.*\)$' -> '<PROJECT_ROOT>/components/\1'
module.name_mapper='^uikit\(.*\)$' -> '<PROJECT_ROOT>/uikit/\1'
module.name_mapper='^static\(.*\)$' -> '<PROJECT_ROOT>/static/\1'
module.name_mapper.extension='gql' -> '<PROJECT_ROOT>/global/utils/gqlShim'

[lints]
deprecated-utility=off