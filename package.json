    "dotenv": "^7.0.0",
    "dotenv-webpack": "^1.7.0",
    "element-resize-detector": "^1.1.15",
    "emotion-theming": "^10.0.10",
    "express": "^4.16.4",
    "filesize": "^6.1.0",
    "graphql": "^14.2.1",
    "graphql-import": "^0.7.1",
    "i": "^0.3.6",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.0",
    "jsdom": "^14.0.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.14",
