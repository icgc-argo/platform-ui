{
  "name": "argo-platform-ui",
  "version": "1.9.14",
  "scripts": {
    "dev": "next -p 8080",
    "build": "next build",
    "storybook": "NODE_PATH=./ start-storybook -s ./ -p 6006",
    "build-storybook": "build-storybook",
    "create-component": "plop --plopfile ./uikit/plopfile.js",
    "gitmoji-config": "gitmoji -g",
    "commit": "gitmoji -c",
    "generate-fragment-types": "ts-node ./scripts/tsconfig.json ./scripts/generateFragmentTypes.ts",
    "type-check-scripts": "tsc -p ./scripts/tsconfig.json",
    "type-check": "tsc -p ./tsconfig.json",
    "test-uikit": "NODE_PATH=./ ts-mocha -p ./tsconfig.test.json ./testSetup.tsx uikit/testSetup.tsx uikit/**/*.test.tsx",
    "test": "NODE_PATH=./ ts-mocha -p ./tsconfig.test.json ./testSetup.tsx components/**/*.test.tsx global/**/*.test.tsx gqlValidation/compilation.test.ts",
    "test:watch": "NODE_PATH=./ ts-mocha --watch -p ./tsconfig.test.json ./testSetup.tsx  ./testSetup.tsx components/**/*.test.tsx global/**/*.test.tsx",
    "test-gql-validation": "ts-mocha -p ./tsconfig.test.json ./testSetup.tsx ./gqlValidation/queryValidations.test.ts",
    "build-uikit::assets": "cp ./uikit/package.release.json ./uikit/dist/package.json && cp -r ./uikit/assets ./uikit/dist/assets && cp ./uikit/readme.md ./uikit/dist/readme.md && cp ./uikit/.npmrc ./uikit/dist/.npmrc",
    "build-uikit::ts": "tsc -p ./uikit/tsconfig.json",
    "build-uikit::babel": "cp ./uikit/.babelrc.json ./uikit/dist/.babelrc && babel ./uikit/dist --out-dir ./uikit/dist && rm ./uikit/dist/.babelrc",
    "build-uikit::cleanup": "rm ./uikit/dist/**/*.jsx ./uikit/dist/*.jsx",
    "build-uikit": "rm -rf ./uikit/dist && npm run build-uikit::ts && npm run build-uikit::babel && npm run build-uikit::assets && npm run build-uikit::cleanup",
    "publish-uikit": "cd uikit/dist && npm publish --access public"
  },
  "bin": {
    "tsc": "./node_modules/.bin/tsc",
    "babel": "./node_modules/@babel/cli/bin/babel.js"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
